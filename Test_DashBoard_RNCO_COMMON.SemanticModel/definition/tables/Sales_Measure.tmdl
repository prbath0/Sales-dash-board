	measure TotalWorkingDays = ```
			
			CALCULATE (
			    COUNTROWS ( 'Date_Table_03' ),
			    Date_Table_03[Year] = YEAR(TODAY()),  -- Only current year
			    Date_Table_03[Month] = MONTH(TODAY()),  -- Only current month
			    Date_Table_03[Weekday] <= 6,       -- Exclude Sundays
			    Date_Table_03[IsPoya] = FALSE()    -- Exclude Poya days
			)
			
			
			
			```
		formatString: 0
		lineageTag: 6f24ceca-de6f-4b97-bdf4-e3edffdb7432

	measure WorkingDaysToDate = ```
			
			CALCULATE (
			    COUNTROWS ( 'Date_Table_03' ),
			    Date_Table_03[Date] <= TODAY(),    -- Up to today's date
			    Date_Table_03[Year] = YEAR(TODAY()),  -- Only current year
			    Date_Table_03[Month] = MONTH(TODAY()),  -- Only current month
			    Date_Table_03[Weekday] <= 6,       -- Exclude Sundays
			    Date_Table_03[IsPoya] = FALSE()    -- Exclude Poya days
			)
			
			
			
			```
		formatString: 0
		lineageTag: dc41fec1-3e8f-4513-8b79-1357e3d98567

	measure 'Working Day Percentage' = ```
			
			DIVIDE(
			    [TotalWorkingDays] - [WorkingDaysToDate],
			    [TotalWorkingDays],
			    0
			)
			
			```
		formatString: 0%;-0%;0%
		lineageTag: 456abbcb-c9d9-4207-a07d-dedacb18d22a

	measure 'Working Days Passed (Current Month)' = ```
			
			 [TotalWorkingDays]-[WorkingDaysToDate]
			
			
